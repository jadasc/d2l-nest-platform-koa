{
  "name": "d2l-nest-platform-koa",
  "version": "0.2.0",
  "description": "Koa platform for nest",
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "https://github.com/Brightspace/d2l-nest-platform-koa.git",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "nest build",
    "prepare": "npm run build",
