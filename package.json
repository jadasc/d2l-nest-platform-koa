    "@types/koa-views": "^2.0.4",
    "@types/koa__cors": "^2.2.3",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.1",
    "path-to-regexp": "^5.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2"
  },
  "devDependencies": {
    "@koa/cors": "^3.0.0",
    "@nestjs/cli": "^6.9.0",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "class-validator": "^0.11.0",
    "eslint": "6.5.1",
    "eslint-config-brightspace": "^0.6.4",
    "mocha": "^6.2.2",
    "mocha-junit-reporter": "^1.23.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "pug": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "should": "^13.2.3",
    "source-map-support": "^0.5.16",
    "supertest": "^4.0.2",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.5.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test"
    ],
    "reporter": [
      "html"
    ]
  }
}
